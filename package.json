{
    "name": "yonius",
    "version": "0.1.1",
    "description": "The Yonius library",
    "keywords": [
        "api",
        "foundation",
        "js",
        "library",
        "yonius"
    ],
    "homepage": "https://github.com/hivesolutions/ripe-id-api-js#readme",
    "bugs": {
        "url": "https://github.com/hivesolutions/yonius/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hivesolutions/yonius.git"
    },
    "license": "Apache-2.0",
    "author": "Hive Solutions",
    "files": [
        "LICENSE",
        "dist/**/*",
        "res/**/*",
        "js/**/*.js",
        "test/**/*.js"
    ],
    "main": "dist/yonius.cjs.js",
    "unpkg": "dist/yonius.umd.js",
    "module": "dist/yonius.esm.js",
    "browser": "dist/yonius.umd.js",
    "scripts": {
        "build": "rollup -c",
        "example": "node examples/simple.js",
        "lint": "eslint \"./{js,test}/**/*.{js,json}\"",
        "prettier": "prettier \"./*.{js,json}\" \"./{js,test}/**/*.{js,json}\" --write",
        "test": "mocha --recursive",
        "upgrade": "npx sort-package-json && ncu -u",
        "watch": "rollup -c -w"
    },
    "dependencies": {
        "node-fetch": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.0",
        "@babel/preset-env": "^7.4.2",
        "eslint": "^5.15.3",
        "eslint-config-hive": "^0.1.8",
        "mocha": "^6.0.2",
        "mocha-cli": "^1.0.1",
        "npm-check-updates": "^3.1.1",
        "prettier": "^1.16.4",
        "prettier-config-hive": "^0.1.1",
        "rollup": "^1.7.3",
        "rollup-plugin-commonjs": "^9.2.2",
        "rollup-plugin-node-resolve": "^4.0.1",
        "sort-package-json": "^1.22.0"
    }
}
