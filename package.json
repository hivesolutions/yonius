{
    "name": "yonius",
    "version": "0.11.3",
    "description": "The Yonius library",
    "keywords": [
        "api",
        "foundation",
        "js",
        "library",
        "yonius"
    ],
    "homepage": "https://github.com/hivesolutions/yonius#readme",
    "bugs": {
        "url": "https://github.com/hivesolutions/yonius/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hivesolutions/yonius.git"
    },
    "license": "Apache-2.0",
    "author": "Hive Solutions",
    "exports": {
        ".": {
            "import": "./dist/yonius.esm.mjs",
            "require": "./dist/yonius.cjs.js",
            "types": "./types/index.d.ts",
            "typings": "./types/index.d.ts"
        },
        "./fastify": {
            "import": "./dist/yonius.fastify.esm.mjs",
            "require": "./dist/yonius.fastify.cjs.js",
            "types": "./types/fastify/index.d.ts",
            "typings": "./types/fastify/index.d.ts"
        }
    },
    "main": "dist/yonius.cjs.js",
    "unpkg": "dist/yonius.umd.js",
    "module": "dist/yonius.esm.js",
    "browser": "dist/yonius.umd.js",
    "types": "types/index.d.ts",
    "import": "dist/yonius.esm.mjs",
    "require": "dist/yonius.cjs.js",
    "typesVersions": {
        "*": {
            "*": [
                "types/index.d.ts"
            ],
            "fastify": [
                "types/fastify/index.d.ts"
            ]
        }
    },
    "typings": "types/index.d.ts",
    "files": [
        "LICENSE",
        "dist/**/*",
        "res/**/*",
        "js/**/*.js",
        "test/**/*.js",
        "types/**/*.d.ts"
    ],
    "scripts": {
        "build": "rollup -c",
        "coveralls": "npm run test-coverage && nyc report --reporter=text-lcov | coveralls",
        "example": "node examples/simple.js",
        "lint": "eslint rollup.config.js \"./{js,types,test}/**/*.{js,ts,json}\"",
        "lint-fix": "eslint rollup.config.js \"./{js,types,test}/**/*.{js,ts,json}\" --fix",
        "prettier": "prettier rollup.config.js \"./*.{js,ts,json}\" \"./{js,types,test}/**/*.{js,ts,json}\" --write",
        "pretty": "npm run prettier && npm run lint-fix",
        "test": "mocha --recursive",
        "test-coverage": "nyc npm test",
        "upgrade": "npx sort-package-json && ncu -u",
        "watch": "rollup -c -w"
    },
    "dependencies": {
        "node-fetch": "^2.6.7"
    },
    "devDependencies": {
        "@babel/core": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "coveralls": "^3.1.1",
        "eslint": "^7.24.0",
        "eslint-config-hive": "^0.5.3",
        "fastify": "^3.27.1",
        "mocha": "^9.2.0",
        "mocha-cli": "^1.0.1",
        "mongoose": "^5.13.14",
        "npm-check-updates": "^12.3.0",
        "nyc": "15.1.0",
        "prettier": "^2.5.1",
        "prettier-config-hive": "^0.1.7",
        "rollup": "^2.67.2",
        "rollup-plugin-dts": "^4.1.0",
        "rollup-plugin-node-polyfills": "^0.2.1",
        "sort-package-json": "^1.54.0",
        "typescript": "^4.5.5"
    }
}
